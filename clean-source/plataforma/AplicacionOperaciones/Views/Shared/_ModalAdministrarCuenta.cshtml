<div class="modal fade" id="modalAdminCuenta" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title family-teamwork">Administración de Cuenta</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="row">
                            <div class="col-12 ta-center">
                                @{
                                        Html.RenderPartial("_ClientesInformacion", new ViewDataDictionary(){
                                        { "Tamaño", "100px" },
                                        { "ColorEvent", "success" },
                                        { "Codigo", "" },
                                        { "Id", "" }
                                    });
                                }
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12" style="font-size: 30px; ">
                        <p  id="NombreCliente" class="modal-title family-teamwork ta-center fw-bold dspl-block"></p>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 color-150x3" style="font-size: 24px;">
                        <p id="Empresa" class="modal-title family-teamwork ta-center"></p>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 color-150x3" style="font-size: 24px; ">
                        <p id="Rut" class="modal-title family-teamwork text-center"></p>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12" style="font-size: 24px;">

                        <p class="modal-title family-teamwork text-center ">KAM Asignado</p>
                        <p id="Nombre" class="modal-title family-teamwork text-center"></p>

                    </div>
                </div>
                <br><br>
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 ta-center">
                        <a href="#" class="eventDescartarAnulacion btn btn-teamwork ta-left color-fx3 br-25 height-50 pdr-30">
                            <i class="glyphicon-tw-6 glyphicon-tw-check dspl-inlblc mb-15neg"></i>
                            <span class="dspl-inlblc">
                                <i class="family-teamwork dspl-blc fs-14 fst-normal mb-8neg">Asociar</i>
                                <i class="family-teamwork dspl-blc fs-14 fst-normal tipoAnulacion">Cuenta</i>
                            </span>
                        </a>

                        <a href="#" class="eventDescartarAnulacion btn btn-warning ta-left color-fx3 br-25 height-50 pdr-30">
                            <i class="glyphicon-tw-6 glyphicon-tw-cancel dspl-inlblc mb-15neg"></i>
                            <span class="dspl-inlblc">
                                <i class="family-teamwork dspl-blc fs-14 fst-normal mb-8neg">Descartar</i>
                                <i class="family-teamwork dspl-blc fs-14 fst-normal tipoAnulacion">Cuenta</i>
                            </span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>