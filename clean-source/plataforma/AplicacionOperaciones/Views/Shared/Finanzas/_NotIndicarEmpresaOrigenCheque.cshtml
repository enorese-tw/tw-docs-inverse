<div class="col-12 col-sm-12 col-md-12 col-lg-12 ta-center">
    <div style="width: 60px; height: 60px; border-radius: 50%; background-size: 50%;" class="success dspl-inline-block glyphicon-tw-check"></div>
    <div style="width: 100px; height: 5px;" class="success dspl-inline-block"></div>
    <div style="width: 60px; height: 60px; border-radius: 50%; background-size: 50%;" class="success dspl-inline-block glyphicon-tw-check"></div>
    <div style="width: 100px; height: 5px;" class="success dspl-inline-block"></div>
    <div style="width: 60px; height: 60px; border-radius: 50%; background-size: 50%;" class="success dspl-inline-block glyphicon-tw-check"></div>
    <div style="width: 100px; height: 5px;" class="success dspl-inline-block"></div>
    <div style="width: 60px; height: 60px; border-radius: 50%; background-size: 50%;" class="danger dspl-inline-block glyphicon-tw-excel"></div>
</div>
<div class="col-12 col-sm-12 col-md-12 col-lg-12 ta-center">
    <div style="width: 100px; border-radius: 50%; font-size: 12px;" class="dspl-inline-block family-teamwork">
        Selección de Tipo de Pago
    </div>
    <div style="width: 60px; height: 5px;" class="dspl-inline-block"></div>
    <div style="width: 100px;border-radius: 50%; font-size: 12px;" class="dspl-inline-block family-teamwork">
        Incorporación de Cheques
    </div>
    <div style="width: 60px; height: 5px;" class="dspl-inline-block"></div>
    <div style="width: 100px; border-radius: 50%; font-size: 12px;" class="dspl-inline-block family-teamwork">
        Confirmación de Incorporaciones
    </div>
    <div style="width: 60px; height: 5px;" class="dspl-inline-block"></div>
    <div style="width: 100px; border-radius: 50%; font-size: 12px;" class="dspl-inline-block family-teamwork">
        Obtención de Archivo de Cheques
    </div>
</div>
<div class="col-12 col-sm-12 col-md-12 col-lg-12">
    <h1 class="family-teamwork"><i class="glyphicon-tw-5 glyphicon-tw-cheque"></i> Proceso de pago por Cheque: No se puede continuar</h1>
</div>
<div class="col-lg-1 col-md-1"></div>
<div class="col-12 col-sm-12 col-md-10 col-lg-10 mt-30 ta-center">
    <div class="row">
        <div class="col-12 col-sm-12 col-md-12 col-lg-12">
            <img src="~/Resources/cancel.png" style="width: 70px; border-radius: 50%;" class="danger dspl-block m-auto" />
            <h1 class="family-teamwork ta-center color-70x3">No se puede continuar con el proceso de pago por</h1>
            <h1 class="family-teamwork ta-center color-70x3 mt-10neg" style="font-size: 50px;">CHEQUE</h1>
            <h4 class="family-teamwork ta-center color-70x3 mt-10neg">Debido a que no hay finiquitos incluidos en este proceso.</h4>
            <p class="family-teamwork ta-center">Puedes volver alguna de las etapas anteriores del proceso</p>
            <div class="ta-center">
                @using (Ajax.BeginForm("ReturnStageInicial", "Finanzas", new AjaxOptions()
                {
                    HttpMethod = "POST",
                    InsertionMode = InsertionMode.Replace,
                    UpdateTargetId = "procesoPago",
                    LoadingElementId = "loader"
                }))
                {
                    <button type="submit" class="btn btn-teamwork"
                            style="border-radius: 25px; height: 40px; text-align: left; color: #fff;">
                        <i class="glyphicon-tw-7 glyphicon-tw-actualizar" style="display: inline-block;"></i>
                        <span style="display: inline-block; margin-top: -7px;">
                            <i style="font-family: 'Open Sans Condensed', sans-serif;
                                font-size: 14px; font-style: normal; display: block; margin-bottom: -8px;">Volver a Seleccionar</i>
                            <i style="font-family: 'Open Sans Condensed', sans-serif;
                                        font-size: 14px; font-style: normal; display: block;">Metodo de Pago</i>
                        </span>
                    </button>
                }
                @using (Ajax.BeginForm("ReturnStageIncorporacionCheques", "Finanzas", new AjaxOptions()
                {
                    HttpMethod = "POST",
                    InsertionMode = InsertionMode.Replace,
                    UpdateTargetId = "procesoPago",
                    LoadingElementId = "loader"
                }))
                {
                    <input type="text" name="codigoProceso" value="@ViewBag.ReferenciaCodigoProceso" class="dspl-none" />
                    <button type="submit" class="btn btn-danger"
                            style="border-radius: 25px; height: 40px; text-align: left; color: #fff;">
                        <i class="glyphicon-tw-7 glyphicon-tw-reasignar" style="display: inline-block;"></i>
                        <span style="display: inline-block; margin-top: -7px;">
                            <i style="font-family: 'Open Sans Condensed', sans-serif;
                                            font-size: 14px; font-style: normal; display: block; margin-bottom: -8px;">Volver a Inclusión de</i>
                            <i style="font-family: 'Open Sans Condensed', sans-serif;
                                                    font-size: 14px; font-style: normal; display: block;">Cheques en Proceso</i>
                        </span>
                    </button>
                }
                @using (Ajax.BeginForm("ReturnStageConfirmacionCheques", "Finanzas", new AjaxOptions()
                {
                    HttpMethod = "POST",
                    InsertionMode = InsertionMode.Replace,
                    UpdateTargetId = "procesoPago",
                    LoadingElementId = "loader"
                }))
                {
                    <input type="text" name="codigoProceso" value="@ViewBag.ReferenciaCodigoProceso" class="dspl-none" />
                    <button type="submit" class="btn btn-danger"
                            style="border-radius: 25px; height: 40px; text-align: left; color: #fff;">
                        <i class="glyphicon-tw-7 glyphicon-tw-reasignar" style="display: inline-block;"></i>
                        <span style="display: inline-block; margin-top: -7px;">
                            <i style="font-family: 'Open Sans Condensed', sans-serif;
                                                font-size: 14px; font-style: normal; display: block; margin-bottom: -8px;">Volver a Confirmación de</i>
                            <i style="font-family: 'Open Sans Condensed', sans-serif;
                                                        font-size: 14px; font-style: normal; display: block;">Cheques en Proceso</i>
                        </span>
                    </button>
                }
            </div>

        </div>
    </div>

</div>
<div class="col-lg-1 col-md-1"></div>