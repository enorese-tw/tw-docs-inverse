@foreach (AplicacionOperaciones.Models.Finanzas.IndiceCheque item in ViewBag.RenderizadoIndicesCheques)
{
    <div class="row">
        <div class="col-12 col-sm-12 col-md-12 col-lg-12">
            <p class="family-teamwork color-428BCA" style="font-size: 25px;">Empresa Origen</p>
            <h1 class="family-teamwork color-428BCA mt-15neg" style="font-size: 40px;">@item.EmpresaOrigen</h1>
        </div>
        <div class="col-12 col-sm-12 col-md-12 col-lg-12">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-6 col-lg-6">
                    <p class="family-teamwork color-428BCA fw-bold ta-center" style="font-size: 40px;">Cheques Totales</p>
                    <h1 class="family-teamwork color-428BCA fw-bold mt-15neg ta-center" style="font-size: 60px;">@item.Totales</h1>
                </div>
                <div class="col-12 col-sm-12 col-md-6 col-lg-6">
                    <p class="family-teamwork color-2178379 fw-bold" style="font-size: 20px;">Cheques Emitidos</p>
                    <h1 class="family-teamwork mt-15neg color-2178379">@item.Emitidos</h1>
                    <p class="family-teamwork color-9218492 fw-bold" style="font-size: 20px;">Cheques Disponibles</p>
                    <h1 class="family-teamwork mt-15neg color-9218492">@item.Disponibles</h1>
                </div>
            </div>
        </div>
    </div>
}