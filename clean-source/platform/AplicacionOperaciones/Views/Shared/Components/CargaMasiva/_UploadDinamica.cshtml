@using Teamwork.Model.Operaciones
@model List<ProcesoCargaMasiva>

@foreach (ProcesoCargaMasiva item in Model)
{
    <div class="col-12 col-sm-12 col-md-12 col-lg-12 ta-center">
        <input type="button" id="eventFileUpload" ng-click="uploadExcel()" style="display: none;" />
        <input type="file" id="file" style="display: none;" accept="application/excel" />
        <a href="#" id="fileUpload" class="btn @item.UploadRenderizadoColor" data-mameTemplate="@item.UploadNombrePlantilla" data-nameSheet="@item.UploadNombreHojaCargaMasiva"
           data-cifrado="@item.UploadCifrado" data-columnas="@item.UploadColumnas" data-typetemplate=""
           data-nodoPadre="@item.UploadNodoPadre" data-nodoHijo="@item.UploadNodoHijo" data-templatedownload="@item.DownloadCifrado"
           style="border-radius: 50px; height: 80px; min-width: 400px; margin-top: 12px; text-align: left; color: #fff;">
            <i class="glyphicon-tw-15 @item.UploadRenderizadoGlyphicon" style="display: inline-block;"></i>
            <span style="display: inline-block; margin-top: -68px;">
                <i style="font-family: 'Open Sans Condensed', sans-serif; font-size: 25px; font-style: normal; display: block; margin-bottom: -10px;">@item.UploadRenderizadoOneTexto</i>
                <i style="font-family: 'Open Sans Condensed', sans-serif; font-size: 25px; font-style: normal; display: block;">@item.UploadRenderizadoSecTexto</i>
            </span>
        </a>
        <p class="new-family-teamwork mt-5" style="font-weight: 100; font-size: 15px;">@item.UploadRenderizadoMensajeImpt</p>
    </div>
}
