                                        OnSuccess = "ModalPaymentShow",
                                        LoadingElementId = "loader"
                                    }))
                                    {
                                        <input type="text" class="dspl-none" name="pago" value="@item.Pago" />
                                        <input type="text" class="dspl-none" name="codigoPago" value="@item.CodigoPago" />
                                        <input type="text" class="dspl-none" name="paginationIndex" value="@ViewBag.ReferenciaPaginationIndex" />
                                        <input type="text" class="dspl-none" name="origen" value="CMP" />
                                        <input type="text" class="dspl-none" name="onlyFiniquito" value="@ViewBag.OnlyFiniquito" />
                                        <button type="submit" class="btn btn-anulado"
                                                style="border-radius: 25px; height: 40px; margin-top: 12px; text-align: left; color: #fff;">
                                            <i class="glyphicon-tw-7 glyphicon-tw-reasignar" style="display: inline-block;"></i>
                                            <span style="display: inline-block; margin-top: -7px;">
                                                <i style="font-family: 'Open Sans Condensed', sans-serif;
                                                            font-size: 14px; font-style: normal; display: block; margin-bottom: -8px;">Revertir Solicitud</i>
                                                <i style="font-family: 'Open Sans Condensed', sans-serif;
                                                            font-size:
                                                                14px; font-style: normal; display: block;">Transferencia/Deposito</i>
                                            </span>
                                        </button>
                                    }
                                }

                                @if ((string)item.ConfirmarPago == "S")
                                {
                                    using (Ajax.BeginForm("ConsultarTipoConfirmacion", "Bajas", new AjaxOptions()
                                    {
                                        HttpMethod = "POST",
                                        InsertionMode = InsertionMode.Replace,
                                        UpdateTargetId = "contentPayment",
                                        OnSuccess = "ModalPaymentShow",
                                        LoadingElementId = "loader"
                                    }))
                                    {
                                        <input type="text" class="dspl-none" name="enlaceViewCaratula" value="@item.EnlaceDocumento" />
                                        <input type="text" class="dspl-none" name="paginationIndex" value="@ViewBag.ReferenciaPaginationIndex" />
                                        <input type="text" class="dspl-none" name="codigoComplemento" value="@item.CodigoComplemento" />
                                        <input type="text" class="dspl-none" name="origen" value="CMP" />
                                        <button type="submit" class="btn btn-teamwork"
                                                style="border-radius: 25px; height: 40px; margin-top: 12px; text-align: left; color: #fff;">
                                            <i class="glyphicon-tw-7 glyphicon-tw-asignado" style="display: inline-block;"></i>
                                            <span style="display: inline-block; margin-top: -7px;">
                                                <i style="font-family: 'Open Sans Condensed', sans-serif;
                                                    font-size: 14px; font-style: normal; display: block; margin-bottom: -8px;">Confirmar el</i>
                                                <i style="font-family: 'Open Sans Condensed', sans-serif;
                                                    font-size: 14px; font-style: normal; display: block;">Pago de Complemento</i>
                                            </span>
                                        </button>
                                    }
                                }
                            </td>
                        </tr>
                    }
                </tbody>
            </table>

            <div class="mt-40 mb-100 ta-center">
                @Html.Partial("Teamwork/_PaginationsComplementos")
            </div>
        }
        else
        {
            <img src="~/Resources/sinresultados.png" style="width: 100px;" class="dspl-block m-auto" />
            <h1 class="family-teamwork ta-center color-70x3 mt-10ne">Ups, Lo sentimos! No se ha encontrado lo que esta buscando!</h1>
            <h1 class="family-teamwork ta-center color-70x3 mt-10neg" style="font-size: 50px;">Sistema de Complementos</h1>
        }

        break;
}
