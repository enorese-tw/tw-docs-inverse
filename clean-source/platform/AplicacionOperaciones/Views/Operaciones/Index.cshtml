                            {
                                Html.RenderPartial("_DashboardSolicitudes", new ViewDataDictionary() {
                                { "Concepto", item.Concepto },
                                { "TipoProceso", item.TipoProceso },
                                { "Total", item.Total },
                                { "Twest", item.Twest },
                                { "Twrrhh", item.Twrrhh },
                                { "Twc", item.Twc },
                                { "UsuarioContratos", item.Analista }
                            });

                            }
                            break;
                        case "Kam":
                            Html.RenderPartial("_DashboardKam");
